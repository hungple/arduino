<bc_settings>
  <rules>
    <list name="VerViewer">
      <str_val name="RulesClass" value="TPluginRules"/>
    
      <str_val name="FileTypes" value="*.exe;*.dll;*.sys;*.vxd;*.drv;*.ocx;*.386;*.scr;*.bcp"/>
    
      <str_val name="Version" value=""/>
    
      <str_val name="HandledBy" value="Version Viewer"/>
    </list>
  
    <list name="PixViewer">
      <str_val name="RulesClass" value="TPluginRules"/>
    
      <str_val name="FileTypes" value="*.jpg;*.jpeg;*.gif;*.bmp;*.png"/>
    
      <str_val name="Version" value=""/>
    
      <str_val name="HandledBy" value="Picture Viewer"/>
    </list>
  
    <list name="MP3Viewer">
      <str_val name="RulesClass" value="TPluginRules"/>
    
      <str_val name="FileTypes" value="*.mp3;*.mpeg3"/>
    
      <str_val name="Version" value=""/>
    
      <str_val name="HandledBy" value="MP3 Viewer"/>
    </list>
  
    <list name="HexViewer">
      <str_val name="RulesClass" value="TPluginRules"/>
    
      <str_val name="FileTypes" value="*.bin"/>
    
      <str_val name="Version" value=""/>
    
      <str_val name="HandledBy" value="Hex Viewer"/>
    </list>
  
    <list name="DataViewer">
      <str_val name="RulesClass" value="TPluginRules"/>
    
      <str_val name="FileTypes" value="*.csv"/>
    
      <str_val name="Version" value=""/>
    
      <str_val name="HandledBy" value="Data Viewer"/>
    </list>
  
    <list name="C/C++/C# Source">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.cpp;*.hpp;*.c;*.h;*.cs"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="4"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <int_val name="Case" value="1"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="/*%00*/%00M"/>
      
        <wstr_val name="-Delim" value="//%00%00"/>
      
        <wstr_val name="+Delim" value="%22%00%22%00E\"/>
      
        <wstr_val name="+Delim" value="'%00'%00E\"/>
      </list>
    </list>
  
    <list name="Comma Separated Values">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.csv"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="+Delim" value="%22%00%22%00M"/>
      </list>
    </list>
  
    <list name="Delphi Source">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.pas;*.dpr"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="4"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="{%00}%00M"/>
      
        <wstr_val name="-Delim" value="(*%00*)%00M"/>
      
        <wstr_val name="-Delim" value="//%00%00"/>
      
        <wstr_val name="+Delim" value="'%00'%00"/>
      
        <list name="LW">
          <wstr_val name="E" value="^\s*begin\s*$"/>
        
          <int_val name="W" value="5"/>
        </list>
      
        <list name="LW">
          <wstr_val name="E" value="^\s*end;?\s*^"/>
        
          <int_val name="W" value="5"/>
        </list>
      
        <list name="LW">
          <wstr_val name="E" value="^\s*(procedure|function)"/>
        
          <int_val name="W" value="500"/>
        </list>
      
        <list name="LW">
          <wstr_val name="E" value="=\s*class\s*\(.*\)"/>
        
          <int_val name="W" value="1000"/>
        </list>
      </list>
    </list>
  
    <list name="Delphi Forms">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.dfm"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="4"/>
    
      <int_val name="Speed" value="5"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="+Delim" value="'%00'%00"/>
      </list>
    </list>
  
    <list name="General Text">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.txt"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <int_val name="Case" value="1"/>
    
      <str_val name="WS" value="-B"/>
    
      <list name="Classify"/>
    </list>
  
    <list name="HTML">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.htm;*.html"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="%3C!--%00--%3E%00M"/>
      
        <wstr_val name="+Delim" value="%22%00%22%00M"/>
      </list>
    </list>
  
    <list name="Java">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.java;*.jav"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <int_val name="Case" value="1"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="/*%00*/%00M"/>
      
        <wstr_val name="-Delim" value="//%00%00"/>
      
        <wstr_val name="+Delim" value="%22%00%22%00E\"/>
      </list>
    </list>
  
    <list name="Python">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.py"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="5"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-TEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="#%00%00"/>
      
        <wstr_val name="+Delim" value="%22%00%22%00M"/>
      
        <list name="LW">
          <wstr_val name="E" value="^\s*def\s+.*\(.*\)"/>
        
          <int_val name="W" value="500"/>
        </list>
      </list>
    </list>
  
    <list name="Registry Dump">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.reg;*.ini"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="+Delim" value="%22%00%22%00M"/>
      
        <list name="LW">
          <wstr_val name="E" value="^\[.*\]$"/>
        
          <int_val name="W" value="1000"/>
        </list>
      </list>
    </list>
  
    <list name="UTF8 Text">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.utf"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="65001"/>
    
      <int_val name="Case" value="1"/>
    
      <str_val name="WS" value="-B"/>
    
      <list name="Classify"/>
    </list>
  
    <list name="Visual Basic">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.bas;*.frm;*.cls;*.vbp;*.ctl;*.vbs"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="5"/>
    
      <int_val name="CodePage" value="0"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="'%00%00"/>
      
        <wstr_val name="+Delim" value="%22%00%22%00M"/>
      
        <list name="LW">
          <wstr_val name="E" value="^(public|private)?\s*(static)?\s*(sub|function)\s*\(.*\)"/>
        
          <int_val name="W" value="500"/>
        </list>
      </list>
    </list>
  
    <list name="XML">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.xml;*.xsl"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="4"/>
    
      <int_val name="Speed" value="5"/>
    
      <int_val name="CodePage" value="0"/>
    
      <int_val name="Case" value="1"/>
    
      <str_val name="WS" value="-LTEB"/>
    
      <list name="Classify">
        <wstr_val name="-Delim" value="%3C!--%00--%3E%00M"/>
      
        <wstr_val name="+Delim" value="%22%00%22%00M"/>
      </list>
    </list>
  
    <list name="Everything Else">
      <str_val name="RulesClass" value="TFreeForm1Rules"/>
    
      <str_val name="FileTypes" value="*.*"/>
    
      <str_val name="Version" value=""/>
    
      <int_val name="Tab" value="8"/>
    
      <int_val name="Speed" value="6"/>
    
      <int_val name="CodePage" value="0"/>
    
      <int_val name="Case" value="1"/>
    
      <str_val name="WS" value="-B"/>
    
      <list name="Classify"/>
    </list>
  </rules>
</bc_settings>
